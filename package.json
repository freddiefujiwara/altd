{
  "name": "altd",
  "version": "0.0.2",
  "description": "Access log tail dispatcher",
  "type": "module",
  "bin": {
    "altd": "./index.js"
  },
  "main": "./src/altd.js",
  "exports": {
    ".": "./src/altd.js"
  },
  "files": [
    "src",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freddiefujiwara/altd.git"
  },
  "keywords": [
    "access log",
    "httpd",
    "log"
  ],
  "author": "Fumikazu Fujiwara <npm@ze.gs> (http://freddiefujiwara.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freddiefujiwara/altd/issues"
  },
  "homepage": "https://github.com/freddiefujiwara/altd#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "nodejs-tail": "^1.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "vitest": "^2.1.8"
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "reporter": [
        "text",
        "html",
        "lcov"
      ],
      "exclude": [
        "node_modules",
        "test"
      ]
    }
  }
}
